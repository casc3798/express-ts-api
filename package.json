    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.8",
    "mailgun.js": "^3.5.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "pg": "^8.7.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "uid-generator": "^2.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
